{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "certSecretNames": {
            "type": "array"
        },
        "configMap": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "enabled": {
                    "type": "boolean"
                },
                "useFullname": {
                    "type": "boolean"
                }
            }
        },
        "deployment": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                }
            }
        },
        "extraManifests": {
            "type": "array"
        },
        "fullnameOverride": {
            "type": "string"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "ingress": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "enabled": {
                    "type": "boolean"
                },
                "hosts": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "path": {
                    "type": "string"
                },
                "tls": {
                    "type": "array"
                }
            }
        },
        "livenessProbe": {
            "type": "object",
            "properties": {
                "delay": {
                    "type": "integer"
                },
                "path": {
                    "type": "string"
                },
                "period": {
                    "type": "integer"
                }
            }
        },
        "nameOverride": {
            "type": "string"
        },
        "nodeSelector": {
            "type": "object"
        },
        "podAnnotations": {
            "type": "object"
        },
        "podLabels": {
            "type": "object"
        },
        "replicaCount": {
            "type": "integer"
        },
        "resources": {
            "type": "object"
        },
        "securityContext": {
            "type": "object"
        },
        "containerSecurityContext": {
            "type": "object"
        },
        "service": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "labels": {
                    "type": "object"
                },
                "port": {
                    "type": "integer"
                },
                "type": {
                    "type": "string"
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                },
                "imagePullSecrets": {
                    "type": "array"
                }
            }
        },
        "serviceMonitor": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "interval": {
                    "type": "string"
                },
                "additionalLabels": {
                    "type": "object"
                },
                "annotations": {
                    "type": "object"
                },
                "metricRelabelings": {
                    "type": "array"
                },
                "relabelings": {
                    "type": "array"
                }
            }
        },
        "tolerations": {
            "type": "array"
        }
    }
}
