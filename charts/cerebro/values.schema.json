{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "config": {
            "type": "object",
            "properties": {
                "basePath": {
                    "type": "string"
                },
                "hosts": {
                    "type": "array"
                },
                "restHistorySize": {
                    "type": "integer"
                },
                "secret": {
                    "type": "string"
                },
                "tlsVerify": {
                    "type": "boolean"
                }
            }
        },
        "deployment": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "labels": {
                    "type": "object"
                },
                "livenessProbe": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        }
                    }
                },
                "podAnnotations": {
                    "type": "object"
                },
                "podLabels": {
                    "type": "object"
                },
                "readinessProbe": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        }
                    }
                }
            }
        },
        "env": {
            "type": "null"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "ingress": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "enabled": {
                    "type": "boolean"
                },
                "hosts": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "labels": {
                    "type": "object"
                },
                "path": {
                    "type": "string"
                },
                "tls": {
                    "type": "array"
                }
            }
        },
        "nodeSelector": {
            "type": "object"
        },
        "priorityClassName": {
            "type": "string"
        },
        "replicaCount": {
            "type": "integer"
        },
        "resources": {
            "type": "object"
        },
        "revisionHistoryLimit": {
            "type": "integer"
        },
        "securityContext": {
            "type": "object",
            "properties": {
                "runAsGroup": {
                    "type": "integer"
                },
                "runAsNonRoot": {
                    "type": "boolean"
                },
                "runAsUser": {
                    "type": "integer"
                }
            }
        },
        "service": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "labels": {
                    "type": "object"
                },
                "port": {
                    "type": "integer"
                },
                "type": {
                    "type": "string"
                }
            }
        },
        "tolerations": {
            "type": "array"
        }
    }
}
